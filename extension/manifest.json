{
  "name": "Disable Ctrl+Q",
  "description": "Disable the Ctrl+Q shortcut",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },  
  "commands": {
    "toggle": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "MacCtrl+Q",
        "linux": "Ctrl+Q"
      },
      "description": "Map Ctrl+Q to the toggle command"
    }
  }
}
